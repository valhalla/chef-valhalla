{
  "mjolnir": {
    "concurrency": <%= node[:valhalla][:mjolnir][:concurrency] %>,
    "input": {
      "type": "protocolbuffer"
    },
    "hierarchy": {
      "tile_dir": "<%= node[:valhalla][:tile_dir] %>",
      "levels": [
        {"name": "local", "level": 2, "size": 0.25},
        {"name": "arterial", "level": 1, "size": 1, "importance_cutoff": "Tertiary"},
        {"name": "highway", "level": 0, "size": 4, "importance_cutoff": "Trunk"}
      ]
    },
    "tagtransform": {
      "node_script": "<%= node[:valhalla][:conf_dir] %>/vertices.lua",
      "node_function": "nodes_proc",
      "way_script": "<%= node[:valhalla][:conf_dir] %>/edges.lua",
      "way_function": "ways_proc",
      "relation_script": "<%= node[:valhalla][:conf_dir] %>/edges.lua",
      "relation_function": "rels_proc"
    },
    "admintagtransform": {
      "node_script": "<%= node[:valhalla][:conf_dir] %>/admins.lua",
      "node_function": "nodes_proc",
      "way_script": "<%= node[:valhalla][:conf_dir] %>/admins.lua",
      "way_function": "ways_proc",
      "relation_script": "<%= node[:valhalla][:conf_dir] %>/admins.lua",
      "relation_function": "rels_proc"
    },
    "admin": {
      "admin_dir": "<%= node[:valhalla][:tile_dir] %>",
      "db_name": "admin.sqlite"
    },
    "logging": {
      "type": "file",
      "file_name": "<%= node[:valhalla][:log][:mjolnir] %>",
      "reopen_interval": "60"
    }
  },
  "thor": {
    "logging": {
      "type": "file",
      "file_name": "<%= node[:valhalla][:log][:thor] %>",
      "reopen_interval": "60"
    }
  },
  "tyr": {
    "listen_address": "<%= node[:valhalla][:tyr][:listen_address] %>",
    "port": <%= node[:valhalla][:tyr][:port] %>,
    "logging": {
      "type": "file",
      "file_name": "<%= node[:valhalla][:log][:tyr] %>",
      "reopen_interval": "60"
    }
  },
  "costing_options": {
    "auto": {
      "maneuver_penalty": 5.0,
      "gate_cost": 30.0,
      "toll_booth_cost": 15.0,
      "toll_booth_penalty": 0.0
    },
    "auto_shorter": {
    },
    "pedestrian": {
      "walking_speed": 5.1,
      "walkway_factor": 0.9,
      "alley_factor": 2.0,
      "driveway_factor": 2.0,
      "step_penalty": 30.0
    },
    "bicycle": {
    }
  }
}
