{
  "mjolnir": {
    "concurrency": <%= node[:valhalla][:mjolnir][:concurrency] %>,
    "input": {
      "type": "protocolbuffer"
    },
    "hierarchy": {
      "tile_dir": "<%= node[:valhalla][:tile_dir] %>",
      "levels": [
        {"name": "local", "level": 2, "size": 0.25},
        {"name": "arterial", "level": 1, "size": 1, "importance_cutoff": "Tertiary"},
        {"name": "highway", "level": 0, "size": 4, "importance_cutoff": "Primary"}
      ]
    },
    "tagtransform": {
      "node_script": "<%= node[:valhalla][:conf_dir] %>/vertices.lua",
      "node_function": "nodes_proc",
      "way_script": "<%= node[:valhalla][:conf_dir] %>/edges.lua",
      "way_function": "ways_proc",
      "relation_script": "<%= node[:valhalla][:conf_dir] %>/edges.lua",
      "relation_function": "rels_proc"
    },
    "admintagtransform": {
      "node_script": "<%= node[:valhalla][:conf_dir] %>/admins.lua",
      "node_function": "nodes_proc",
      "way_script": "<%= node[:valhalla][:conf_dir] %>/admins.lua",
      "way_function": "ways_proc",
      "relation_script": "<%= node[:valhalla][:conf_dir] %>/admins.lua",
      "relation_function": "rels_proc"
    },
    "statistics": {
      "statistics_dir": "<%= node[:valhalla][:tile_dir] %>",
      "db_name": "statistics.sqlite"
    },
    "admin": {
      "admin_dir": "<%= node[:valhalla][:tile_dir] %>",
      "db_name": "admin.sqlite"
    },
    "transit": {
      "transit_dir": "<%= node[:valhalla][:tile_dir] %>",
      "db_name": "transit.sqlite"
    },
    "logging": {
      "type": "std_out",
      "color": false
    }
  },
  "loki": {
    "logging": {
      "type": "std_out",
      "color": false
    },
    "service": {
      "proxy": "ipc://<%= node[:valhalla][:conf_dir] %>/loki"
    }
  },
  "thor": {
    "logging": {
      "type": "std_out",
      "color": false
    },
    "service": {
      "proxy": "ipc://<%= node[:valhalla][:conf_dir] %>/thor"
    }
  },
  "odin": {
    "logging": {
      "type": "std_out",
      "color": false
    },
    "service": {
      "proxy": "ipc://<%= node[:valhalla][:conf_dir] %>/odin"
    }
  },
  "tyr": {
    "logging": {
      "type": "std_out",
      "color": false
    },
    "service": {
      "proxy": "ipc://<%= node[:valhalla][:conf_dir] %>/tyr"
    }
  },
  "httpd": {
    "service": {
      "listen": "tcp://<%= node[:valhalla][:httpd][:listen_address] %>:<%= node[:valhalla][:httpd][:port] %>",
      "loopback": "ipc://<%= node[:valhalla][:conf_dir] %>/loopback"
    }
  },
  "service_limits": {
    "max_distance": {
      "auto": 5000000.0,
      "auto_shorter": 5000000.0,
      "bus": 5000000.0,
      "pedestrian": 100000.0,
      "bicycle": 500000.0,
      "multimodal": 500000.0
    },
    "max_route_locations": 8
  },
  "costing_options": {
    "auto": {
      "maneuver_penalty": 5.0,
      "destination_only_penalty": 600.0,
      "alley_penalty": 5.0,
      "gate_cost": 30.0,
      "toll_booth_cost": 15.0,
      "toll_booth_penalty": 0.0,
      "country_crossing_cost": 600.0,
      "country_crossing_penalty": 0.0
    },
    "auto_shorter": {
    },
    "bus": {
    },
    "pedestrian": {
      "walking_speed": 5.1,
      "walkway_factor": 0.9,
      "alley_factor": 2.0,
      "driveway_factor": 2.0,
      "step_penalty": 30.0
    },
    "bicycle": {
    },
    "multimodal": {
    },
    "transit": {
    }
  }
}
